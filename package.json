{
  "name": "eurekacloudapp",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "test": "jest --watch --coverage=false",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "start": "npx expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build-production": "eas build --p all",
    "build-dev": "eas build --profile development -p all",
    "build-preview": "eas build --profile preview -p all",
    "build-production-android": "eas build --platform android",
    "build-dev-android": "eas build -p android --profile development",
    "build-preview-android": "eas build -p android --profile preview",
    "build-production-ios": "eas build --platform ios",
    "build-dev-ios-simulator": "eas build --platform ios --profile ios-simulator",
    "build-dev-ios": "eas build --platform ios --profile development",    
    "build-preview-ios": "eas build --platform ios --profile preview"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@types/i18next": "^13.0.0",
    "@types/react-i18next": "^8.1.0",
    "axios": "^1.7.2",
    "expo": "~51.0.17",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image": "~1.12.12",
    "expo-image-picker": "~15.0.6",
    "expo-linear-gradient": "~13.0.2",
    "expo-location": "~17.0.1",
    "expo-media-library": "~16.0.4",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "i18next": "^23.11.5",
    "i18next-resources-for-ts": "^1.5.0",
    "lodash.clonedeep": "^4.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.1",
    "react-native": "0.74.3",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-drawer-layout": "^3.3.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-paper": "^5.12.3",
    "react-native-paper-dates": "^0.22.10",
    "react-native-reanimated": "~3.10.1",
    "react-native-root-toast": "^3.6.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.10",
    "expo-dev-client": "~4.0.19",
    "expo-splash-screen": "~0.27.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "prettier": "^3.3.2",
    "typescript": "~5.3.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  }
}
